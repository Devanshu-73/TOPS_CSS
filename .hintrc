{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "justify-content: space-evenly",
          "background-color: #8067739b",
          "position: sticky",
          "text-decoration-line",
          "text-decoration-color",
          "text-decoration-style",
          "text-decoration-thickness",
          "border-block-end",
          "border-block-start",
          "filter",
          "text-align-last",
          "display: grid",
          "grid-template-columns",
          "grid-row-start",
          "grid-row-end",
          "grid-column-start",
          "grid-column-end",
          "grid-template-rows"
        ]
      }
    ],
    "sri": "off",
    "axe/parsing": [
      "default",
      {
        "marquee": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ]
  }
}